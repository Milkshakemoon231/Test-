-- Scripts:

local function SQLYDF_fake_script() -- Enter.LocalScript 
	local script = Instance.new('LocalScript', Enter)

	local UserData = script.Parent.Parent.Discord -- this is the variable for the 'username' textbox. 
	local button = script.Parent -- this is a variable for the Button.
	local UserPass = script.Parent.Parent.Password -- this is a variable for the 'Password' textbox
	local End = script.Parent.Parent.Parent.AfterGui -- rename Hackz to ur frame u want to open after logging in.
	button.MouseButton1Click:Connect(function() -- will run the script down below after clicking the button
		if UserData.Text == "Milkshake" and UserPass.Text == "M" then -- change noob and 111 to a username and password u want
			End.Visible = true  -- makes the hack frame visible after logging in.
			wait(0) -- waits one second.
			script.Parent.Parent.Visible = false -- makes the login frame invisible after logging in.
		else -- if the pass or username is wrong it will run the script down below
			button.Text = "Invalid Info" -- changes the text to invalid info
			wait(0.5) -- waits 0,5 secs
			button.Text = "Try again" -- changes the text to try again
			wait(0.5) -- waits another 0,5 secs
			button.Text = "Login" -- changes the text back to login
		end -- ends the if statement
	end) -- ends the button function
	
	
	-- fully scripted by NTO EXPLOITS on youtube -- 
end
coroutine.wrap(SQLYDF_fake_script)()
local function ASPEWHJ_fake_script() -- Enter.LocalScript 
	local script = Instance.new('LocalScript', Enter)

	local function callback(Text)
		if Text == "Button1 text" then
			print ("Answer")
		end
	end
	
	local NotificationBindable = Instance.new("BindableFunction")
	NotificationBindable.OnInvoke = callback
	--
	game.StarterGui:SetCore("SendNotification",  {
		Title = "Welcome to LoginGui!";
		Text = "Enjoy the music üòè";
		Icon = "";
		Duration = 100;
		Button1 = "k";
		Callback = NotificationBindable;
	})
end
coroutine.wrap(ASPEWHJ_fake_script)()
local function JNFPFCN_fake_script() -- GuimadebyMilkshake.LocalScript 
	local script = Instance.new('LocalScript', GuimadebyMilkshake)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent.Parent.Parent.LoginGui
	local dragToggle = nil
	local dragSpeed = 0.1
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
	
end
coroutine.wrap(JNFPFCN_fake_script)()
local function NGCJB_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local Frame = script.Parent.Parent.Parent.AfterGui
	
	script.Parent.MouseButton1Click:Connect(function()
		Frame.Visible = true
	end)
end
coroutine.wrap(NGCJB_fake_script)()
local function HWHKCY_fake_script() -- MUSIC.LocalScript 
	local script = Instance.new('LocalScript', MUSIC)

	--Variables
	local GoodSound = game.SoundService.GoodSound
	local play = false
	
	--script
	script.Parent.MouseButton1Click:Connect(function()
		if play then
			GoodSound.Volume = 1				--volume from the music when it's on
			play = false
			script.Parent.Text = "Click this to stop da stoopid music üíÉ" else  --you can change the text from the button here between ""
			GoodSound.Volume = 0					--volume from the music when it's off
			play = true			
			script.Parent.Text = "Play da music againüíÉ"		--you can change the text from the button here between ""
		end
	end)
end
coroutine.wrap(HWHKCY_fake_script)()
local function TBEOF_fake_script() -- Goodway.LocalScript 
	local script = Instance.new('LocalScript', Goodway)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Parent.AfterGui.Visible = true
	end)
end
coroutine.wrap(TBEOF_fake_script)()
